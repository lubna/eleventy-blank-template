<header class="wrapper">
    <a href="/" rel="home" aria-label="Go To Homepage">
        {{ metadata.title }}
    </a>

    <nav>
        <ul role="list">
            {%- for entry in collections.all | eleventyNavigation %}
                <li>
                    <a href="{{ entry.url | url }}">
                        {{ entry.title }}
                    </a>
                </li>
            {%- endfor %}
        </ul>
    </nav>
</header>